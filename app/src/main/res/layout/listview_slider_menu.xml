<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="80dp"
    android:background="@color/app_white">


    <RelativeLayout
        android:id="@+id/relativeLayout12"
        android:layout_width="match_parent"
        android:layout_height="80dp">

        <emerge.project.onmealoutlet.ui.font.OpenSansLight
            android:id="@+id/textView_slidermenu_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/relativeLayout4"
            android:layout_toRightOf="@+id/relativeLayout4"
            android:freezesText="true"
            android:textColor="@color/text_title"
            android:textAppearance="@android:style/TextAppearance.Medium" />

        <RelativeLayout
            android:id="@+id/relativeLayout4"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp">

            <ImageView
                android:id="@+id/imageView_slider_menu_icon"
                android:layout_width="25dip"
                android:layout_height="25dip"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true" />
        </RelativeLayout>
    </RelativeLayout>


    <View
        android:background="#efefef"
        android:layout_height="1dip"
        android:layout_below="@+id/relativeLayout12"
        android:layout_width="match_parent" />


</RelativeLayout>
